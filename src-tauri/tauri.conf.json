{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "decorations": true,
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "Mission Kontrol",
        "width": 800
      }
    ]
  },
  "build": {
    "frontendDist": "../dist-ui",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "identifier": "com.tauri.dev",
  "productName": "mission-kontrol",
  "version": "0.0.1"
}
